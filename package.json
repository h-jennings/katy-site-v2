{
  "name": "katy-site-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "gen": "graphql-codegen --require dotenv/config --config codegen.yml dotenv_config_path=.env.local",
    "dev": "npm run gen && next dev",
    "dev:inspect": "npm run gen && NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:profile": "next build --profile",
    "build:analyze": "cross-env ANALYZE=true next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "upgrade-deps": "ncu -u -i",
    "lint": "next lint",
    "lint:staged": "lint-staged",
    "e2e": "npm run build && playwright test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-toast": "^0.1.1",
    "@radix-ui/react-visually-hidden": "^0.1.4",
    "@stitches/react": "^1.2.8",
    "copy-to-clipboard": "^3.3.1",
    "framer-motion": "^6.3.10",
    "next": "12.1.6",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^5.4.0",
    "next-sitemap": "^3.0.5",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "next-urql": "^3.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ssr-prepass": "^1.5.0",
    "urql": "^2.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "^2.5.0",
    "@graphql-codegen/typescript-operations": "^2.4.1",
    "@graphql-codegen/typescript-urql": "^3.5.11",
    "@next/bundle-analyzer": "^12.1.6",
    "@playwright/test": "^1.22.2",
    "@types/node": "17.0.41",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "dotenv": "^16.0.1",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "npm-check-updates": "^13.1.1",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "^4.7.3"
  }
}
